    "search-bar-placeholder": "Suche Tastenkürzel"
    "table-header-keystroke": "Tastendruck"
    "table-header-command": "Kommando"
    "table-header-source": "Quelle"
    "table-header-selector": "Selektor"
  }
  packages: {
    searchBarText: 'Pakete nach Namen filtern'
  }
  themes: {
    notes: {
      text1: "Atom kann durch editieren "
      text2: "Deines Stylesheets"
      text3: " angepasst werden"   # optional
      _template: "${text1} <a>${text2}</a> ${text3}"
    }
    title1: "UI-Theme"
    description1: "Steuert die Anzeige der Benutzeroberfläche."
    title2: "Syntax-Theme"
