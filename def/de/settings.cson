    searchBarText: 'Themes nach Name filtern'
  }
  updates: {
    "check-updates": "Suche nach Aktualisierungen"
    "update-all": "Alles aktualisieren"
    "checking-updates": "Suche nach Aktualisierungen..."
    "all-updated-message": "Alle deine installierten Pakete sind aktuell!"
  }
