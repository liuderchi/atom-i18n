    "search-bar-placeholder": "Suche Tastenkürzel"
    "table-header-keystroke": "Tastendruck"
    "table-header-command": "Kommando"
    "table-header-source": "Quelle"
    "table-header-selector": "Selektor"
  }
  packages: {
    searchBarText: 'Plugins nach Namen filtern'
  }
  themes: {
    notes: {
      text1: "Atom kann durch Editieren "
      text2: "deines Stylesheets"
      text3: " angepasst werden"   # optional
      _template: "${text1} <a>${text2}</a> ${text3}"
    }
    title1: "UI-Theme"
    description1: "Steuert die Anzeige der Benutzeroberfläche."
    title2: "Syntax-Theme"
    description2: "Steuert die Anzeige (Syntaxhighlighting) des Textes im Editor."
    searchBarText: 'Themes nach Name filtern'
  }
  updates: {
    "check-updates": "Suche nach Aktualisierungen"
    "update-all": "Alle aktualisieren"
    "checking-updates": "Suche nach Aktualisierungen…"
    "all-updated-message": "Alle deine installierten Plugins sind aktuell!"
  }
  install: {
    notes: {
      text1: "Plugins sind auf "
      text2: " veröffentlicht und wurden in "
      text3: " installiert"   # optional
      _template: "${text1}<a>atom.io</a>${text2} /path/to/pkg ${text3}"
    }
    "searchbar-placeholder": "Durchsuche Plugins"
    "btn-packages": "Plugins"
    "btn-themes": "Themes"
  }
  "control-btns":{
    install: "Installieren"
