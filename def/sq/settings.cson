    searchBarText: 'Filtroji temat sipas emrash'
  }
  updates: {
    "check-updates": "Kontrollo për përditësime"
    "update-all": "Përditësoji Krejt"
    "checking-updates": "Po kontrollohet për përditësime..."
    "all-updated-message": "Krejt paketat tuaja të instaluara janë të përditësuara!"
  }
