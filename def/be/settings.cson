			}
			{
				_id: "editor.lineHeight"
				title: "Вышыня радка"
				desc: "Вышыня радкоў рэдактара ў залежнасці ад памеру шрыфту."
			}
			{
				_id: "editor.maxScreenLineLength"
				title: "Максімальная даўжыня радка на экране"
				desc: "Вызначае максімальную шырыню акна рэдактара перад прымусовым мяккім пераносам (у сімвалах)."
			}
			{
				_id: "editor.nonWordCharacters"
				title: "Не буквы"
				desc: "Радок сімвалаў для вызначэння мяжы слова."
			}
			{
				_id: "editor.preferredLineLength"
				title: "Даўжыня радка"
				desc: "Вызначае даўжыню радка, якая выкарыстоўваецца, калі ўключаны <code>Мяккі перанос пры перавышэнні даўжыні радка</code> (у сімвалах)."
			}
			{
				_id: "editor.scrollPastEnd"
				title: "Пракрутка да канца апошняга радка"
				desc: "Дазволіць пракрутку рэдактара да канца апошняга радка."
			}
			{
				_id: "editor.scrollSensitivity"
				title: "Адчувальнасць пракруткі"
				desc: "Вызначае хуткасць пракруткі тэксту з дапамогай мышы альбо трэкпада."
			}
			{
				_id: "editor.showCursorOnSelection"
