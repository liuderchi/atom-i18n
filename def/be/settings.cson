	settings:
		notes: [
			{
				_id: "core-settings-note"
				html: "Гэта асноўныя налады Atom, якія не ўплываюць на параметры рэдагавання тэксту. У пакунках могуць быць асабістыя налады, змяніць іх можна ва ўкладцы <a class=\"link packages-open\">Пакункі</a>."
			}
			{
				_id: "editor-settings-note"
				html: "Гэта параметры рэдагавання тэксту. Некаторыя з іх можна перавызначыць на зыходную мову. Правярайце налады мовы кожнага пакунка ва ўкладцы <a class=\"link packages-open\">Пакункі</a>."
			}
		]
		controls: [
			{
				_id: "core.allowPendingPaneItems"
				title: "Адкрываць пстрычкай"
				desc: "Дазволіць адкрываць файлы адзіночнай пстрычкай па іх у дрэве праекта."
			}
			{
				_id: "core.audioBeep"
				title: "Гукавы сігнал"
				desc: "Даваць гукавы сігнал калі дзеянне не было выканана альбо мэта не дасягнута."
			}
			{
				_id: "core.automaticallyUpdate"
				title: "Аўтаматычнае абнаўленне"
				desc: "Аўтаматычна абнаўляць Atom пры наяўнасці новай версіі."
			}
			{
				_id: "core.autoHideMenuBar"
				title: "Аўтаматычнае хаванне меню"
				desc: "Аўтаматычна хаваць меню і паказваць яго пры націсканні на кнопку Alt. Падтрымліваецца толькі ў Windows &amp; Linux."
			}
			{
				_id: "core.closeDeletedFileTabs"
				title: "Закрываць укладкі адлеглых файлаў"
				desc: "Закрываць акно рэдактара, калі адпаведны файл быў выдалены па-за Atom."
			}
			{
				_id: "core.closeEmptyWindows"
				title: "Закрываць пустыя вокны"
				desc: "Калі ў акне няма адкрытых укладак альбо панэляў, то запусціцца загад 'Закрыць укладку', які закрые акно."
			}
			{
				_id: "core.destroyEmptyPanes"
				title: "Закрываць пустыя панэлі"
				desc: "Калі апошняя ўкладка панэлі будзе закрыта, то панэль закрыецца."
			}
			{
				_id: "core.excludeVcsIgnoredPaths"
				title: "Выключаць шляхі ігнаравання сістэмы кіравання версіямі"
				desc: "Файлы і каталогі сістэмы кіравання версіямі, што будуць ігнаравацца некаторымі пакункамі (напрыклад \"Знайсці і замяніць\")Напрыклад, у праектах з Git шляхі зададзены ў файле .gitignore. У некаторых пакункаў могуць быць свае налады."
			}
			{
				_id: "core.fileEncoding"
				title: "Кадаванне файла"
				desc: "Прадвызначанае кадаванне для запісу і чытання файлаў."
			}
			{
				_id: "core.fileSystemWatcher"
				title: "Прагляд файлавай сістэмы"
				desc: "Абярыце асноўную рэалізацыю, што выкарыстоўваецца для назірання за зменамі файлавай сістэмы. Эмуляцыя змен міне падзеі, выкліканыя іншымі праграмамі, але можа дапамагчы прадухіліць збоі."
				select:
					native: "Стандартныя API аперацыйнай сістэмы"
					atom: "Эмуляцыя з падзеямі Atom"
			}
			{
				_id: "core.followSymlinks"
				title: "Выкарыстоўваць сімвалічныя спасылкі"
				desc: "Выкарыстоўваць сімвалічныя спасылкі падчас пошуку і адкрыцця файлаў у дрэве."
			}
			{
				_id: "core.ignoredNames"
				title: "Не зважаць на назвы"
				desc: "Спіс <code>глабальных назваў</code>. Файлы і каталогі, што суадносяцца з гэтым спісам, будуць ігнаравацца ў спісе каталогаў злева(дрэве файлаў). У некаторых пакункаў могуць быць свае налады."
			}
			{
				_id: "core.openEmptyEditorOnStart"
				title: "Адкрываць пусты рэдактар пры запуску"
				desc: "Аўтаматычна адкрывае пусты рэдактар падчас запуску Atom."
			}
			{
				_id: "core.packagesWithKeymapsDisabled"
				title: "Выключыць пакункі з асабістымі гарачымі клавішамі"
			}
			{
				_id: "core.projectHome"
				title: "Хатні каталог для праектаў"
				desc: "Каталог, дзе размяшчаюцца праекты. Пакункі, створаныя генератарам пакункаў, захоўваюцца там прадвызначана."
			}
			{
				_id: "core.reopenProjectMenuCount"
				title: "Колькасць праектаў у спісе нядаўна закрытых"
				desc: "Абраць колькасць праектаў, што будуць адлюстроўвацца ў меню 'Нядаўна закрытыя'."
			}
			{
				_id: "core.restorePreviousWindowsOnStart"
				title: "Аднаўляць адкрытыя вокны падчас наступнага запуску"
				desc: "Калі адзначана, аднаўляць усе вокны Atom папярэдняга сеансу альбо з загаднага радка <code>atom</code>; , альбо будзе загружаны пусты ліст."
				select:
					no: "не"
					yes: "так"
					always: "заўсёды"
			}
